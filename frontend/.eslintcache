GameRoute>\n            <Route path={`${path}/history`}>\n\n            </Route>\n            <Route path={`${path}/profile`}>\n                \n            </Route>\n        </>\n    )\n}\n\nconst UserHomePage = (props) => {\n    const {url, userId} = props;\n    return (\n        <>\n            <h1 className=\"title glow-on-hover\">GO game</h1>\n            <div className=\"user-menu\">\n                <button className=\"button\" onClick={()=>{findOpponent(userId)}}>\n                    <NavLink className=\"user-menu-text\" to={`${url}/game`}>Find Opponent</NavLink>\n                </button>\n                <button className=\"button\">\n                    <NavLink className=\"user-menu-text\" to={`${url}/history`}>History</NavLink>\n                </button>\n                <button className=\"button\">\n                    <NavLink className=\"user-menu-text\" to={`${url}/profile`}>My Profile</NavLink>\n                </button>\n            </div>\n        </>\n    )\n}\n\nexport {UserMenu};\n","/mnt/document/web/test_GO/frontend/src/axios.js",[],"/mnt/document/web/test_GO/frontend/src/components/line.js",[],"/mnt/document/web/test_GO/frontend/src/components/index.js",[],"/mnt/document/web/test_GO/frontend/src/components/Login.js",["84","85"],{"ruleId":"86","severity":1,"message":"87","line":1,"column":26,"nodeType":"88","messageId":"89","endLine":1,"endColumn":35},{"ruleId":"86","severity":1,"message":"90","line":1,"column":37,"nodeType":"88","messageId":"89","endLine":1,"endColumn":43},{"ruleId":"86","severity":1,"message":"91","line":2,"column":9,"nodeType":"88","messageId":"89","endLine":2,"endColumn":17},{"ruleId":"86","severity":1,"message":"92","line":8,"column":8,"nodeType":"88","messageId":"89","endLine":8,"endColumn":19},{"ruleId":"86","severity":1,"message":"93","line":16,"column":38,"nodeType":"88","messageId":"89","endLine":16,"endColumn":52},{"ruleId":"86","severity":1,"message":"94","line":16,"column":71,"nodeType":"88","messageId":"89",